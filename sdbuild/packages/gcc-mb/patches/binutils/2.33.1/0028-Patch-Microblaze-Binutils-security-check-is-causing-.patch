   if (hdr->sh_size > bfd_get_file_size (abfd))
     {
       /* xgettext:c-format */
@@ -448,6 +449,7 @@ _bfd_elf_parse_attributes (bfd *abfd, Elf_Internal_Shdr * hdr)
       bfd_set_error (bfd_error_invalid_operation);
       return;
     }
